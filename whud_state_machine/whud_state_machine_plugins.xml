    </description>
  </class>

  <class name="whud_switch" type="whud_state_machine::WhudSwitch" base_class_type="whud_state_machine::PluginBase">
    <description>
    Switch on or switch off state machine.
    </description>
  </class>

  <class name="whud_image_client" type="whud_state_machine::WhudImageClient" base_class_type="whud_state_machine::PluginBase">
    <description>
    Control move with whud_vision.
    </description>
  </class>

  <class name="lcwd_choose" type="whud_state_machine::LcwdChoose" base_class_type="whud_state_machine::PluginBase">
    <description>
    Choose the branch of state machine.
    </description>
  </class>

  <class name="lcwd_jump" type="whud_state_machine::LcwdJump" base_class_type="whud_state_machine::PluginBase">
    <description>
    Choose the branch of state machine.
    </description>
  </class>

  <class name="lcwd_wait" type="whud_state_machine::LcwdWait" base_class_type="whud_state_machine::PluginBase">
    <description>
    Wait.
    </description>
  </class>

  <class name="lcwd_empty" type="whud_state_machine::LcwdEmpty" base_class_type="whud_state_machine::PluginBase">
    <description>
    Just used to gei into interrupt task.
    </description>
  </class>

  <class name="whud_rplidar_client" type="whud_state_machine::WhudRplidarClient" base_class_type="whud_state_machine::PluginBase">
